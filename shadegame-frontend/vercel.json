// { "rewrites": [{ "source": "/(.*)", "destination": "/" }]}

{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/svg/(.*)", "dest": "/svg/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}

